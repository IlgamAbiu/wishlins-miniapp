# Environment name (for container isolation)
ENV_NAME=production

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_bot_token_here
VITE_BOT_USERNAME=your_bot_username

# Mini App URL (production domain - without port if using Nginx)
MINIAPP_URL=https://your-domain.com

# Backend API URL (production - without port if using Nginx)
VITE_API_URL=https://your-domain.com/api/v1

# Ports (customize if needed for multiple deployments)
FRONTEND_PORT=3000
BACKEND_PORT=3001
DB_PORT=5432

# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=wishlist

# CORS Origins (add your production domain)
CORS_ORIGINS=["https://your-domain.com","http://localhost:5173","http://localhost:3000"]
